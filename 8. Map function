"""syntax = map(f,function)"""
def sqr(l1):
    return l1-5
list1=[1,2,3,4,5,6]
print(list(map(sqr,list1)))
print(list(map(lambda x: x**2, list1)))

def largestinthree(a,b,c):
    if a>b and a>c:
        print(a)
    elif b>a and b>c:
        print(b)
    elif c>a and c>b:
        print(c)
x=int(input("x: "))
y=int(input("y: "))
z=int(input("z: "))
print(largestinthree(x,y,z))
