str=input("str: ")
num=int(input("num: "))
if num<len(str):
    print(str[:num-1]+str[num+1:])
else:
    print("The number should be positive or less than the lenght of the string")


str1=input()
str2=input()
if len(str1)>1 or len(str2)>1:
    print(str1[1:]+str2[1:])
else:
    print("null")


str3=input("str3: ")
str4=input("str4: ")
if len(str3) > 1 or len(str4) >1:
    print(str3[1:-1]+str4[1:-1])
else:
    print("null")


