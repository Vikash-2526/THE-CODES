print(".....Multi level inheritance........")
class Animal:
    def speak(self):
        print("Animal barking")
class Dog(Animal):
    def bark(self):
        print("dog barking")
class DogChild(Dog):
    def eat(self):
        print("Eating Bread..")

d=Dog()
d.bark()
d.speak()
# a=Animal()
# a.speak()
# a.bark()
v=DogChild()
v.speak()
v.bark()
v.eat()
